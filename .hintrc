{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-filter",
          "backface-visibility",
          "scroll-behavior"
        ]
      }
    ]
  }
}